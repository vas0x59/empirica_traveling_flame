FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Ydefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    // X0 
    // {
    //     type            inletOutlet;
    //     inletValue      uniform 0.00;
    //     value           uniform 0.00;
    // }

    XL
    {
        type            inletOutlet;
        inletValue      uniform 0.0;
        value           uniform 0.0;
    }
    top {
        type  zeroGradient;
    }

    "(border).*"
    {
        type            zeroGradient;
        // value           uniform 0.0;
    }

"(sym).*"
    {
        type            symmetryPlane;
    }
}


