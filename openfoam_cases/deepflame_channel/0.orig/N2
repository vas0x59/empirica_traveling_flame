FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.651;

boundaryField
{
    // X0
    // {
    //     type            inletOutlet;
    //     inletValue      $internalField;
    //     value           $internalField;
    // }
    XL
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    "(border).*"
    {
        type            zeroGradient;
    }


    "(sym).*"
    {
        type            symmetryPlane;
    }
}


