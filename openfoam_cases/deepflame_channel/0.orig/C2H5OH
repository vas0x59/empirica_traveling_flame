FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      C2H5OH;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.07;

boundaryField
{
    //  X0
    // {
    //     type            inletOutlet;
    //     inletValue      $internalField;
    //     value           $internalField;
    // }
    XL
    {
        type            inletOutlet;
        inletValue      $internalField;
        // value           $internalField;
    }
    top {
        type  zeroGradient;
    }
    "(border).*"
    {
        type            zeroGradient;
    }
    border_liquid
    {
        type fixedValue;
        value uniform 0.07;
    }
    "(sym).*"
    {
        type            symmetryPlane;
    }
}


