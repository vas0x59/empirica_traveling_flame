FoamFile
{
    format      ascii;
    class       volScalarField;
    object      Xi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1;

boundaryField
{
    
    ".*"
    {
        type            zeroGradient;
    }
    "(sym).*"
    {
        type            symmetryPlane;
    }
}


